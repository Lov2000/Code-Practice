#include <stdio.h>

int main(void) {
      int n,lucky=0,unlucky=0;
      int array[100];
      
      
      scanf("%d",&n);
      for(int i=0;i<n;i++)
      {
          scanf("%d",&array[i]);
      }
      for(int i=0;i<n;i++)
      {
          if(array[i]%2==0)
          lucky++;
          else
          unlucky++;
      }
      
      if(lucky>unlucky)
      printf("READY FOR BATTLE");
      else
      printf("NOT READY");
      return 0;
    
    
}
    
    

